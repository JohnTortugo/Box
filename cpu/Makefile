	bit32.o \
	bmi32.o \
	string.o \
	paging.o

BX_INCLUDES = ../bochs.h ../config.h


all: libcpu.a

.cc.o:
	$(CXX) -c $(BX_INCDIRS) $(CXXFLAGS) $< -o $@


libcpu.a: $(OBJS)  cpudb/libcpudb.a
	rm -f  libcpu.a
	ar rv $@ $(OBJS)  cpudb/libcpudb.a
	$(RANLIB) libcpu.a

$(OBJS): $(BX_INCLUDES)

$(OBJS64): $(BX_INCLUDES)

cpudb/libcpudb.a:
	cd cpudb && \
	$(MAKE) $(MDEFINES) libcpudb.a
	echo done

	
SUBDIRS = cpudb

clean: local-clean
	for subdir in $(SUBDIRS); do \
	    echo making $@ in $$subdir; \
	    ($(MAKE) -C $$subdir $(MDEFINES) $@) || exit 1; \
	done

dist-clean: local-dist-clean
	for subdir in $(SUBDIRS); do \
	    echo making $@ in $$subdir; \
	    ($(MAKE) -C $$subdir $(MDEFINES) $@) || exit 1; \
	done

local-clean:
	rm -f  *.o
	rm -f  *.a

local-dist-clean: 
	rm -f  Makefile

