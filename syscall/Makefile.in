@SUFFIX_LINE@

srcdir = @srcdir@
VPATH = @srcdir@
top_builddir    = ..
top_srcdir      = @top_srcdir@

SHELL = @SHELL@

@SET_MAKE@

CXX = @CXX@
CXXFLAGS = @CXXFLAGS@ @GUI_CXXFLAGS@

LDFLAGS = @LDFLAGS@
LIBS = @LIBS@
X_LIBS = @X_LIBS@
X_PRE_LIBS = @X_PRE_LIBS@
RANLIB = @RANLIB@


BX_INCDIRS = -I.. -I$(srcdir)/.. -I../@INSTRUMENT_DIR@ -I$(srcdir)/../@INSTRUMENT_DIR@

BX_OBJS = \
	syscall.o

BX_INCLUDES = ../bochs.h ../config.h ../debug.h

all: $(BX_OBJS)

.@CPP_SUFFIX@.o:
	$(CXX) @DASH@c $(BX_INCDIRS) $(CXXFLAGS) @CXXFP@$< @OFP@$@


$(BX_OBJS): $(BX_INCLUDES)

clean:
	@RMCOMMAND@ *.o

dist-clean: clean
	@RMCOMMAND@ Makefile

###########################################
# dependencies generated by
#  gcc -MM -I.. -I../instrument/stubs *.cc | sed 's/\.cc/.@CPP_SUFFIX@/g'
###########################################
syscall.o: syscall.@CPP_SUFFIX@ $(BX_INCLUDES)
